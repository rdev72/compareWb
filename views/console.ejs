 <%- include('partials/loggedInHeader') %>
<a href="/">Main</a>

<form action="/article/add" method="post">
    <label>Tittle</label>
    <input type="text" name="tittle" required><br>
    <label>Description</label>
    <input type="text" name="description"><br>
    <input type="submit" value="ADD">
</form>
<hr>
<ol>
<% userArticles.forEach(e => { %>
    <li>
    <strong><%= e.tittle %></strong>
    <p><%= e.description %></p>
    <a href='article/edit/<%= e._id %>'><button>Edit</button></a>
    <a href='article/delete/<%= e._id %>'><button>Delete</button></a>
    </li>
<% }) %>
</ol>